{
  "name": "mimmi20/mezzio-generic-authorization-acl",
  "type": "library",
  "description": "Provides a laminas-permissions-acl adapter for mezzio-generic-authorization.",
  "keywords": [
    "mezzio",
    "authorization",
    "middleware",
    "psr-7",
    "psr-15",
    "acl"
  ],
  "homepage": "https://github.com/mimmi20/mezzio-generic-authorization-acl",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4.3 || ^8.0.0",
    "ext-mbstring": "*",
    "laminas/laminas-permissions-acl": "^2.8.0",
    "mimmi20/mezzio-generic-authorization": "^2.0.4",
    "psr/container": "^1.1.1 || ^2.0.1",
    "psr/http-message": "^1.0.1"
  },
  "conflict": {
    "container-interop/container-interop": "<1.2.0",
    "mezzio/mezzio-authorization-acl": "*"
  },
  "require-dev": {
    "laminas/laminas-servicemanager": "^3.7.0",
    "mimmi20/coding-standard": "^2.4.2",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^0.12.94",
    "phpstan/phpstan-deprecation-rules": "^0.12.6",
    "phpstan/phpstan-phpunit": "^0.12.22",
    "phpstan/phpstan-strict-rules": "^0.12.10",
    "phpunit/phpunit": "^9.5.8"
  },
  "suggest": {
    "laminas/laminas-servicemanager": "to use the factories"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "config-provider": "Mezzio\\GenericAuthorization\\Acl\\ConfigProvider"
    }
  },
  "autoload": {
    "psr-4": {
      "Mezzio\\GenericAuthorization\\Acl\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MezzioTest\\GenericAuthorization\\Acl\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/mezzio-generic-authorization-acl/issues",
    "source": "https://github.com/mimmi20/mezzio-generic-authorization-acl"
  }
}
