{
  "name": "mimmi20/mezzio-generic-authorization-acl",
  "type": "library",
  "description": "Provides a laminas-permissions-acl adapter for mezzio-generic-authorization.",
  "keywords": [
    "mezzio",
    "authorization",
    "middleware",
    "psr-7",
    "psr-15",
    "acl"
  ],
  "homepage": "https://github.com/mimmi20/mezzio-generic-authorization-acl",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1.3 || ^8.0.0",
    "laminas/laminas-permissions-acl": "^2.7.2",
    "mimmi20/mezzio-generic-authorization": "dev-master",
    "psr/container": "^1.0.0",
    "psr/http-message": "^1.0.1"
  },
  "conflict": {
    "container-interop/container-interop": "<1.2.0",
    "mezzio/mezzio-authorization-acl": "*"
  },
  "require-dev": {
    "infection/infection": "~0.13.6 || ~0.18",
    "mimmi20/coding-standard": "~1.0.5",
    "pepakriz/phpstan-exception-rules": "~0.11.1",
    "phpstan/extension-installer": "~1.0.5",
    "phpstan/phpstan": "~0.12.54",
    "phpstan/phpstan-beberlei-assert": "~0.12.3",
    "phpstan/phpstan-deprecation-rules": "~0.12.5",
    "phpstan/phpstan-phpunit": "~0.12.16",
    "phpunit/phpunit": "~7.5.20 || ~8.5.8 || ~9.4.2"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "config-provider": "Mezzio\\GenericAuthorization\\Acl\\ConfigProvider"
    }
  },
  "autoload": {
    "psr-4": {
      "Mezzio\\GenericAuthorization\\Acl\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MezzioTest\\GenericAuthorization\\Acl\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/mezzio-generic-authorization-acl/issues",
    "source": "https://github.com/mimmi20/mezzio-generic-authorization-acl"
  }
}
