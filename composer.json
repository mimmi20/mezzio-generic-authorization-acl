{
  "name": "mimmi20/mezzio-generic-authorization-acl",
  "description": "Provides a laminas-permissions-acl adapter for mezzio-generic-authorization.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "mezzio",
    "authorization",
    "middleware",
    "psr-7",
    "psr-15",
    "acl"
  ],
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/mezzio-generic-authorization-acl",
  "support": {
    "issues": "https://github.com/mimmi20/mezzio-generic-authorization-acl/issues",
    "source": "https://github.com/mimmi20/mezzio-generic-authorization-acl"
  },
  "require": {
    "php": "~8.3.0 || ~8.4.0 || ~8.5.0",
    "laminas/laminas-permissions-acl": "^2.17.0",
    "mimmi20/mezzio-generic-authorization": "^4.0.0",
    "psr/container": "^1.1.2 || ^2.0.2",
    "psr/http-message": "^1.0.1 || ^2.0"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.29.10",
    "laminas/laminas-servicemanager": "^4.3.0",
    "mimmi20/coding-standard": "^6.0.1",
    "nikic/php-parser": "^5.3.1",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^1.12.9",
    "phpstan/phpstan-deprecation-rules": "^1.2.1",
    "phpstan/phpstan-phpunit": "^1.4.0",
    "phpunit/phpunit": "^11.5.2",
    "rector/rector": "^1.2.10",
    "rector/type-perfect": "^1.0.0",
    "symfony/process": "^7.2.0",
    "symplify/phpstan-rules": "^13.0.1",
    "tomasvotruba/cognitive-complexity": "^0.2.3",
    "tomasvotruba/type-coverage": "^1.0.0",
    "tomasvotruba/unused-public": "^1.1.0"
  },
  "conflict": {
    "container-interop/container-interop": "<1.2.0",
    "mezzio/mezzio-authorization-acl": "*"
  },
  "suggest": {
    "laminas/laminas-servicemanager": "to use the factories"
  },
  "autoload": {
    "psr-4": {
      "Mimmi20\\Mezzio\\GenericAuthorization\\Acl\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20\\Mezzio\\GenericAuthorization\\Acl\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "config-provider": "Mimmi20\\Mezzio\\GenericAuthorization\\Acl\\ConfigProvider"
    }
  }
}
